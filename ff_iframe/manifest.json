{
  "name": "LoadIFrameLocal",
  "description": "LoadIFrameLocal.",
  "version": "0.7.7",
  "author": "test",
  "short_name": "LoadIFrameLocal",
  "browser_specific_settings": {
    "gecko": {
      "id": "{4b8f35f0-6235-448b-a2c2-028fa3e88d66}",
      "strict_min_version": "42.0"
    }
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon19.png",
      "38": "images/icon38.png"
    },
    "default_title": "LoadIFrameLocal"
  },
  "background": {
    "persistent": true,
    "scripts": ["js/background.js"]
  },
  "content_security_policy": "script-src 'self' https://apis.google.com/; object-src 'self'",
  "web_accessible_resources": [
    "html/iframe.html"
  ],
  "manifest_version": 2,
  "permissions": [
    "<all_urls>",
    "contextMenus",
    "idle",
    "storage",
    "webNavigation",
    "identity",
    "contextMenus",
    "clipboardRead",
    "clipboardWrite",
    "webRequest",
    "activeTab",
    "http://*/",
    "https://*/"
  ]
}
